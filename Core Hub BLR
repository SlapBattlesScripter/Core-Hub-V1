local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
	Name = "Core | Hub",
	LoadingTitle = "Core | Hub",
	LoadingSubtitle = "by blue_s0u1 on discord",
	ConfigurationSaving = {
		Enabled = false,
		FolderName = nil,
		FileName = "Core Hub"
	},
	Discord = {
		Enabled = true,
		Invite = "https://discord.gg/zjjuB7cBuj",
		RememberJoins = true
	},
	KeySystem = false,
	KeySettings = {
		Title = "Key | Core Hub",
		Subtitle = "Key System",
		Note = "Key In https://discord.gg/zjjuB7cBuj",
		FileName = "TestKey",
		SaveKey = true,
		GrabKeyFromSite = true,
		Key = {"BLUELOCKISASS"}
	}
})
local MiscTab = Window:CreateTab("Styles", nil)

local MiscSection = MiscTab:CreateSection("Custom Styles")

local PushButton = MiscTab:CreateButton({
	Name = "Vexbolts",
	Callback = function()
		local player = game.Players.LocalPlayer
		local playerGui = player:WaitForChild("PlayerGui")
		local styleGui = playerGui:WaitForChild("Style")
		local bgFrame = styleGui:WaitForChild("BG")

		local styleTxt = bgFrame:WaitForChild("StyleTxt")
		local descTxt = bgFrame:WaitForChild("Desc")
		styleTxt.Text = "Vexbolts"
		descTxt.Text = "The sigma that was mass unfollowed"

		local slotsFrame = bgFrame:WaitForChild("Slots")
		local scrollingFrame = slotsFrame:WaitForChild("ScrollingFrame")
		local slot2 = scrollingFrame:WaitForChild("Slot2")
		local slot2Text = slot2:WaitForChild("Text")
		slot2Text.Text = "Vexbolts"
	end
})

local PushButton = MiscTab:CreateButton({
	Name = "Tralalero Tralala",
